{
  "name": "titanfall-chronicles",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "cd server && node index.js",
    "generate:titans": "npx tsx scripts/generate-titans.ts",
    "generate:terrain": "npx tsx scripts/generate-terrain.ts",
    "generate:all": "npx tsx scripts/generate-all.ts",
    "generate:quick": "npx tsx scripts/generate-all.ts --quick",
    "generate:banana": "npx tsx scripts/generate-unified.ts --backend=banana",
    "check:fal": "npx tsx scripts/check-models.ts",
    "test:banana": "npx tsx scripts/banana-client.ts"
  },
  "dependencies": {
    "@banana-dev/banana-dev": "^6.2.0",
    "@fal-ai/client": "^1.9.0",
    "express": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
